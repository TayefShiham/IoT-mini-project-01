config BOARD
    default "rpi-pico-w" if BOARD_RPI_PICO_W

config BOARD_RPI_PICO_W
    bool
    default y
    select CPU_MODEL_RP2040
    select HAS_PERIPH_UART

source "$(RIOTBOARD)/rpi-pico/Kconfig"
